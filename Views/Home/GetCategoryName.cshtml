@model IEnumerable<Medimall.Models.Category>


    @foreach (var item in Model)
    {
        <a class="span-item" href="@Url.Action("ViewProductForCategory","Home",new {id=item.CategoryName})">
            <img src="~/Vendor/img/categories/@(item.CategoryId).png" alt="" />
            <p>@item.CategoryName</p>
        </a>
    }

